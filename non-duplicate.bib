  month={Sep.},}@INPROCEEDINGS{10919451,
  author={Qian, Xiaohu and Yan, Xiaojun and Yin, Mingqiang and Li, Chenglong},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Bi-objective Location Planning Problem of Last-mile Logistics under Low-carbon Emissions}, 
  year={2024},
  volume={},
  number={},
  pages={641-646},
  abstract={With the rapid growth of e-commerce, customers' logistics demands have increased tremendously. To promote the service level of last-mile logistics, a bi-objective location planning problem of the last-mile logistics is particularly investigated under the context of low-carbon emissions. By analyzing the problem characteristics, a bi-objective programming model is constructed. In specific terms, the first objective aims to minimize the overall cost of facility establishment, operation, and transportation, while the second objective seeks to minimize the carbon emissions generated by various energy consumption sources. Employing a genetic algorithm, the feasibility of the proposed model is demonstrated through a random numerical example. By adjusting the weights assigned to the first objective of the overall cost, different solutions can be obtained for practical applications.},
  keywords={Energy consumption;Costs;Transportation;Carbon dioxide;Programming;Planning;Numerical models;Electronic commerce;Logistics;Genetic algorithms;location planning;last-mile logistics;bi-objective;low-carbon emissions},
  doi={10.1109/CSIS-IAC63491.2024.10919451},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{11161169,
  author={Xu, Xinyu and Liu, Ye},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Earlier Times in Front: Regardless of Comparison Sequence, Temporal Focus or Temporal Relative Span}, 
  year={2025},
  volume={},
  number={},
  pages={339-345},
  abstract={To conceptualize time, humans systematically anchor abstract temporal relations to spatial schemas, a phenomenon termed spatiotemporal metaphorization. Within sagittal-axis temporal mappings, sequential relations have been widely observed to follow an earlier-front/later-behind mapping cross-linguistically. However, critical gaps persist regarding how three factors- (1) task-induced comparison sequences, (2) individual differences in temporal focus (past vs. future oriented), as well as (3) temporal span scale and temporal relative distance- modulate this mapping. This study investigated Mandarin speakers' sequential space-time mappings through a design that carefully manipulated comparison sequences and temporal spans (age-, hour-, and festival-related terms). Participants' temporal focus was assessed via a standardized temporal focus questionnaire. Results demonstrated robust adherence to the earlier-front/later-behind mapping across all conditions: neither comparison sequence, temporal span, nor temporal focus significantly influenced response congruency. These findings suggest that sequential temporal mappings operate through an allocentric reference frame, independent of egocentric perspective-taking. However, the mechanism underlying sequential temporal relationship remains unclear, demanding more future studies This study may have practical implications for intelligence systems in the fields of human-like temporal reasoning and intelligent control systems.},
  keywords={Cognition;Spatiotemporal phenomena;Complex systems;Intelligent control;Sequential time;Space-time mapping;Mandarin;compatibility effects},
  doi={10.1109/CSIS-IAC65538.2025.11161169},
  ISSN={},
  month={May},}@INPROCEEDINGS{11161753,
  author={Chen, Yanwei and Tan, Fuxiao},
