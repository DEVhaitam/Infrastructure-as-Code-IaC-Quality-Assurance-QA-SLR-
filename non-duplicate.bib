  author={Alkalbani, Abdullah Said and Mantoro, Teddy},
  booktitle={2017 Second International Conference on Informatics and Computing (ICIC)}, 
  title={Security comparison between dynamic & static WSN for 5g networks}, 
  year={2017},
  volume={},
  number={},
  pages={1-4},
  abstract={In the recent years, Wireless Sensor Networks (WSN) and its applications have obtained considerable momentum. However, security and power limits of these networks are still important matters as security and power limits remain an important problem in WSN. This paper contributes to provide a simulation-based analysis of the energy efficiency, accuracy and path length of static and dynamic wireless sensor networks for 5G environment. Results are analyzed and discussed to show the difference between these two types of sensor networks. The static networks more accurate than dynamic networks. Data move from source to destination in shortest path in dynamic networks compared to static ones.},
  keywords={Wireless sensor networks;Power demand;5G mobile communication;Security;Vehicle dynamics;Monitoring;Energy consumption;WSN;Power Consumption;Static Network;Dynamic Network;5G},
  doi={10.1109/IAC.2017.8280615},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{8780453,
  author={Sugiartawan, Putu and Hartati, Sri},
  booktitle={2018 Third International Conference on Informatics and Computing (ICIC)}, 
  title={Group Decision Support System to Selection Tourism Object in Bali Using Analytic Hierarchy Process (AHP) and Copeland Score Model}, 
  year={2018},
  volume={},
  number={},
  pages={1-6},
  abstract={This paper purpose to combine the GDSS framework with AHP and Copeland Score Model. The framework model is used to ranking popular tourism object in Bali. Tourism became one of the main factors in the development of an area. One of the famous tourist destinations in Indonesia is the province of Bali. Bali becomes a world tourism destination because of its culture and there are hundreds of tourism objects in Bali spread in every district. Each tourist object has beautiful characteristics and panoramas but not all tourist object have a high level of visits. There is an deferences between one tourist objects with the other, affecting the number of tourist visits. To overcome these problems by knowing the characteristics each tourism object that is done through the ranking process. The assessment and ranking process involves various parties related to tourism objects such as tourism, community, academia, government, and entrepreneurs. The model used is the Analytic Hierarchy Process (AHP) algorithm, which is used to rank tourism objects individually and combine ranking results, using the Copeland score model. In the Copeland score model, each object has a weight that is different from each other. The next ranking becomes a reference for improving the assessment criteria of each tourism object. The results showed that the tourism object of Uluwatu is the first rank, with an assessment of 8.1 and Taman Ayun obtaining the last rank with a value of 1. Analysis of sensitivity and risk by changing the characteristic weights on the criteria by giving a high point has an impact on ranking changes.},
  keywords={Object oriented modeling;Analytic hierarchy process;Mathematical model;Decision support systems;Analytical models;Indexes;AHP;Copeland Score;Tourism Object;Decision Support System;Group Decision},
  doi={10.1109/IAC.2018.8780453},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10646454,
  author={Caprolu, Maurantonio and Di Pietro, Roberto and Lombardi, Flavio and Onofri, Elia},
  booktitle={2024 IEEE International Conference on Decentralized Applications and Infrastructures (DAPPS)}, 
  title={Characterizing Polkadot’s Transactions Ecosystem: methodology, tools, and insights}, 
  year={2024},
  volume={},
  number={},
  pages={51-60},
  abstract={The growth potential of a crypto project, typically sustained by an associated cryptocurrency, can be measured by the use cases spurred by the underlying technology. However, these projects are implemented through decentralized applications, with a weak (if any) feedback scheme. Hence, a metric that is widely used as a proxy for the healthiness of such projects is the number of transactions and related volumes. Nevertheless, such a metric can be subject to manipulation—the crypto market being an unregulated one, magnifies such a risk. To address the cited gap, in this paper, we design a comprehensive methodology to process large cryptocurrency transaction graphs that, after clustering user addresses of interest, derives a compact representation of the network that highlights interactions among clusters. The analysis of these interactions provides insights into/over/on the strength of the project.To show the quality and viability of our solution, we bring forward a use case centered on Polkadot. The Polkadot network, a cutting-edge cryptocurrency platform, has gained significant attention in the digital currency landscape due to its pioneering approach to interoperability and scalability. However, little is known about how many and to what extent its wide range of enabled use cases have been adopted by end-users so far. The answer to this type of question means mapping Polkadot (or any analyzed crypto project) on a palette that ranges from a thriving ecosystem to a speculative coin without compelling use cases.Our findings, rooted on extensive experimental results—we have parsed 12.5+ million blocks—, demonstrate that crypto exchanges exert considerable influence on the Polkadot network, owning nearly 40% of all addresses in the ledger and absorbing at least 80% of all transactions. In addition, the high volume of inter-exchange transactions (more than 20%) underscores the strong interconnections among just a couple of prominent exchanges, prompting further investigations into the behavior of these actors to uncover potential unethical activities, such as wash trading.These results are a testament to the quality and viability of the proposed solution that, while characterized by a high level of scalability and adaptability, is at the same time immune from the drawbacks of currently used metrics.},
  keywords={Measurement;Visualization;Upper bound;Protocols;Online banking;Scalability;Ecosystems;Blockchain Technology;Cryptocurrencies;Centralized Exchanges;Decentralized Applications;Graph Contraction;Network Analyses;Polkadot},
  doi={10.1109/DAPPS61106.2024.00016},
  ISSN={2835-3498},
  month={July},}@INPROCEEDINGS{8780501,
  author={Afrizal, Sandra Hakiem and Handayani, Putu Wuri and Eryando, Tris and Sartono, Arief},
  booktitle={2018 Third International Conference on Informatics and Computing (ICIC)}, 
  title={Primary Care Functional Requirements of a Health Information System in Indonesia}, 
  year={2018},
  volume={},
  number={},
  pages={1-7},
  abstract={Primary Health Care (PHC) serves not only the primary care service but also the community health service. With such complexity, it is necessary to ensure the quality of health care equally on both sides by implementing Health Information Technology to engage both activities. The purpose of this study is to elaborate functional requirements for the Primary Health Care (PHC) business process. Thus, it may offer a foundation for the government for standardising a PHC Information System and also provide a complete structure of Health Information System (HIS) requirements for the PHC basic process: individual care business process and community health care process. This research is a qualitative study by conducting semi-structured in-depth interviews and observations in two public PHC, the District Health Office of Kota Tangerang Selatan, Provincial Health Office of Banten Province, and the Indonesian Ministry of Health. The discussions were transcribed and analysed using the framework matrix analysis method. The result of this research is the functional requirements of an integrated HIS for primary care in Indonesia.},
  keywords={Information systems;Interviews;Pediatrics;Drugs;Government;primary health care (PHC);health information system (HIS);functional requirements},
