  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={HSS-UNet: A Hybrid State Space Reconstruction Network for Anomaly Detection of Catenary Fasteners}, 
  year={2025},
  volume={},
  number={},
  pages={1-8},
  abstract={Anomaly detection of catenary support components (CSCs) is a crucial aspect of railway condition monitoring systems. However, because the abnormal features of CSC loosening are not immediately apparent, and current CNN models and visual Transformer models face challenges such as limited long-range modeling capabilities and high computational complexity, existing deep learning anomaly detection methods struggle to effectively perform. The state space model (SSM), represented by Mamba, excels at long-range modeling while maintaining linear computational complexity. In this paper, we propose a novel visual state space reconstruction network (HSS-UNet) for detecting CSC loosening anomalies. The network consists of a pre-trained encoder and a Mamba decoder with a multi-scale (position-enhanced state space) LSS module. The HSS block utilizes a (hybrid scan) HS encoder to encode the feature map into five scanning methods and eight orientations, thereby strengthening global connectivity through the state-space model (SSM). Finally, a stable anomaly score calculation module is designed using MLP to assess the degree of anomaly in the components. Experimental results show that the HSS-UNet model, learning strategy, and anomaly score calculation method proposed in this paper are effective and reasonable, offering certain advantages. Therefore, the method proposed can be effectively applied to the detection of CSC abnormalities.},
  keywords={Deep learning;Visualization;Shape;Computational modeling;Fasteners;Vectors;Decoding;Computational efficiency;Computational complexity;Anomaly detection;Catenary Fasteners;Abnormal detection;deep learning;the state space model (SSMs)},
  doi={10.1109/CSIS-IAC65538.2025.11162001},
  ISSN={},
  month={May},}@INPROCEEDINGS{10919452,
  author={Zhang, Zhi and Duan, Fangwei and Sun, Jiazheng and Liu, Hengyu and Shi, Jingrui and Hu, Dawei and Yang, Yingxuan},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={An Electric Vehicle Dispatching Strategy to Improve Power Quality for Vulnerable Nodes in Distribution Network}, 
  year={2024},
  volume={},
  number={},
  pages={287-292},
  abstract={Because that the charging load of large-scale electric vehicles is random and disorderly, an electric vehicle optimization control strategy is proposed based on the combination of power grid economic operation and traffic information. Firstly, the weak links of the distribution network are analyzed, that is, the vulnerable nodes are identified. Based on this, the electric vehicle dispatching control framework is constructed. Then, an optimal control strategy for electric vehicles is formulated for the multi-objective optimization of user demand cost and economic operation cost of power grid. Finally, through the simulation of the actual line equivalent 33 node model, the electric vehicle scheduling strategy improves the power quality of the vulnerable nodes of the power grid. Through the study of cases, the implementation of the scheduling strategy reduces the peak-valley gap of the power grid.},
  keywords={Costs;Power quality;Optimal control;Optimal scheduling;Distribution networks;Electric vehicles;Power grids;Regulation;Dispatching;Voltage control;electric vehicle;vulnerable nodes;electric power quality;optimization control strategy},
  doi={10.1109/CSIS-IAC63491.2024.10919452},
