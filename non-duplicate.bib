@INPROCEEDINGS{8280652,
  author={Ramadhani, Anissa Lintang and Musa, Purnawarman and Wibowo, Eri Prasetyo},
  booktitle={2017 Second International Conference on Informatics and Computing (ICIC)}, 
  title={Human face recognition application using pca and eigenface approach}, 
  year={2017},
  volume={},
  number={},
  pages={1-5},
  abstract={Face is the most memorable part of the body in real life that makes it an important variable. In this research, we use face recognize method that integrated in Ry-UJI robot. The robot is recognized by a detected voice command looking for someone and when a person's face has been found, face recognition is complete. This article will apply the human face recognition system using the eigenface approach. Eigenface is one of the facial recognition methods based on the Principal Component Analysis (PCA) algorithm. PCA involved a mathematical procedure to derive a set of features for face recognition. Face recognition stage begins with face detection process using cascade classifier method, face preprocess, collect and train the face detected and finally the face recognition.},
  keywords={Face;Face recognition;Principal component analysis;Face detection;Eigenvalues and eigenfunctions;Image reconstruction;Testing;C++;Eigenface;Face Detection;Face Recognition;OpenCV;PCA},
  doi={10.1109/IAC.2017.8280652},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{10919326,
  author={Huang, Jiajun and Jiang, Xuejun and Liu, Yi},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={CAGK-YOLO: An Efficient Detection Model for Micro-Scale and Irregular Surface Defects Based on Deep Learning}, 
  year={2024},
  volume={},
  number={},
  pages={425-430},
  abstract={Surface defect detection is an indispensable step in the production and manufacturing process. However, existing methods are susceptible to imaging noise and often struggle to achieve satisfactory results in recognizing micro-scale and irregular defects. This paper proposes an efficient detection model CAGK-YOLO for real-time mechanical components micro-scale and irregular surface defects detection. Firstly, the Coordinate Attention (CA) module is added to the original YOLOv5 to augment the feature extraction capability for defect images. Then, we utilize the K-means algorithm to align the initial anchor frames of the vanilla YOLOv5 model more closely with the distribution of original defects, thereby enhancing the model's ability to detect irregular defects. Lastly, we replace the original three convolutions module in the backbone of YOLOv5 with the Ghost module to accelerate the defect detection process. We also integrate an up-sampling mechanism to further refine the accuracy in detecting micro-scale defects. The experimental results show that the CAGK-YOLO achieves superior performance with comparison models on the benchmark NEU-DET, NEU-Micro, and Samsung oil stain datasets. Moreover, ablation experiments are delivered to verify the validity of CAGK-YOLO's substructures.},
  keywords={YOLO;Deep learning;Strips;Accuracy;Oils;Noise;Production;Real-time systems;Surface treatment;Defect detection;surface defect detection;CAGK-YOLO;deep learning;micro-scale and irregular},
  doi={10.1109/CSIS-IAC63491.2024.10919326},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10641499,
  author={Pignatti, S. and Carfora, M. F. and Coluzzi, R. and D’Amato, L. and De Feis, I. and Mora, D. Fonnegra and Laneve, G. and Imbrenda, V. and Lanfredi, M. and Mirzaei, S. and Palombo, A. and Pascucci, S. and Rossi, F. and Santini, F. and Simoniello, T. and Vanguri, R},
  booktitle={IGARSS 2024 - 2024 IEEE International Geoscience and Remote Sensing Symposium}, 
  title={Detection of Critical Areas Prone to Land Degradation Using Prisma: The Metaponto Coastal Area in South Italy Test Case}, 
  year={2024},
  volume={},
  number={},
  pages={1063-1066},
  abstract={Land cover, or the biophysical cover of the earth's surface, plays an essential role in climate and environmental dynamics. Processes involving land cover change, are among the factors that most threaten the ecosystems sustainability and services. The objective of the work is to explore the potential of the PRISMA multi-temporal hyperspectral imagery in generating new EO products to complement/improve the products provided by Copernicus' Land Monitoring Service for the analysis and monitoring of complex and fragile ecosystems such as the coastal Metaponto (Southern Italy) by estimating of the land biological and economic productivity loss and land degradation vulnerability. Preliminary results showed that an improvement in ecosystem mapping is supported by the use of Artificial Neural Networks (ANN), k-Nearest Neighbors (KNN) and Support Vector Machines (SVM) and a hybrid approach to define the vegetation trait, leads to significant improvement in the damage assessment and land degradation assessment.},
  keywords={Degradation;Support vector machines;Image resolution;Ecosystems;Land surface;Vegetation mapping;Sea measurements;PRISMA;land degradation;vegetation traits;spectral index},
  doi={10.1109/IGARSS53475.2024.10641499},
  ISSN={2153-7003},
  month={July},}@INPROCEEDINGS{10916477,
