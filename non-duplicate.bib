  month={Sep.},}@INPROCEEDINGS{11161896,
  author={Ou, Jiajun and Zeng, Weiliang and Chen, Siyu and Lu, Siyuan},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={YOLO-DeepSORT Empowered UAV-based Traffic Behavior Analysis: A Novel Trajectory Extraction Tool for Traffic Moving Objects at Intersections}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={Accurate detection and tracking of diverse road users-ranging from pedestrians and cyclists to larger vehicles-in mixed urban traffic remains a significant challenge. A UAV-based framework is introduced that integrates a novel MHA-YOLOv8 detector with an Improved DeepSORT tracker. MHA-YOLOv8 enhances the YOLOv8 architecture through multi-head and coordinate attention modules, enabling robust identification of both small and occluded targets under complex intersection conditions. Building upon DeepSORT, the tracking module incorporates a Flyover Tracking feature that adaptively handles prolonged occlusions. A UAV-captured high-precision aerial dataset with RTK calibration, serves to rigorously evaluate the approach in scenarios with dense traffic, overlapping trajectories, and varied user types. Experimental results demonstrate detection accuracy (MAP) of up to 93.56% and a tracking accuracy (MOTA) of $\mathbf{9 8. 6 3 \%}$, even in congested urban intersections. The framework provides a reliable and efficient solution for real-time traffic monitoring, offering valuable insights to support urban planning, traffic safety, and intelligent transportation systems.},
  keywords={Target tracking;Accuracy;Pedestrians;Roads;Motorcycles;Detectors;Real-time systems;Robustness;Trajectory;Monitoring;Small object detection;Deep learning;YOLOv8;Occluded target tracking;Unmanned aerial vehicles},
  doi={10.1109/CSIS-IAC65538.2025.11161896},
  ISSN={},
  month={May},}@INPROCEEDINGS{11161543,
  author={Liu, Sixun and Zhang, Xuan and Xie, Jinyu and Li, Junkun and Cai, Xidong and Wang, Jinjian},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Multi-Mode MPC-Based Control of Exhaust Temperature and Load in a Gas Turbine}, 
  year={2025},
  volume={},
  number={},
  pages={127-133},
  abstract={In the heavy-duty gas turbine field, there are strong coupling and intense nonlinearity between the exhaust temperature and the load, leading to severe challenges in system analysis and controller design for conventional control-based methods. In this paper, an optimal control approach based on multi-mode model predictive control ($\mathrm{M}^{3} \text{PC}$) is proposed. Firstly, a dynamic component-level plant model of gas turbine is established, and the discretization and linearization methods are adopted to convert the nonlinear system into a local linearized model. According to the foregoing theoretical models, a $\mathbf{M}^{\mathbf{3}} \mathbf{P C}$ framework is developed to analyze the prediction of state and output, and the process of receding horizon optimization. Then a multi-mode switching criterion is designed to configure the model updating strategy by exploiting the relationship between the offline calibrated control error and the load percentage to keep the control error suppressed within a small threshold range. Furthermore, the method of blending the control inputs from adjacent modes is employed to smooth and eliminate transient oscillations near the mode switching point. Finally, simulation is implemented to compare the control effects of the $\mathbf{M}^{\mathbf{3}} \mathbf{P C}$ controller and the traditional PI controller under rapid load increase and decrease conditions within the full load range, which indicates that, compared with the traditional PI controller, the $\mathbf{M}^{3} \mathbf{P C}$ controller achieves more than $\mathbf{8 0 \%}$ suppression on the tracking error, and reduces the coupling degree by more than 4 times. Therefore, the $\mathbf{M}^{\mathbf{3}} \mathbf{P C}$ approach proposed in this paper can simultaneously realize high-precision, fast and decoupled control of exhaust temperature and load, providing a solution with both real-time and robustness for the wide operating conditions control of heavy-duty gas turbines.},
  keywords={Couplings;PI control;Switches;Robustness;Temperature control;Transient analysis;Turbines;Oscillators;Load modeling;Predictive control;gas turbine;MPC;exhaust temperature control;load control;decoupling control},
  doi={10.1109/CSIS-IAC65538.2025.11161543},
  ISSN={},
  month={May},}@INPROCEEDINGS{10919309,
  author={He, Xinyi and An, Tianjiao and Cao, Yu and Ma, Bing and Dong, Hongbo and Dong, Bo},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Decentralized Self-triggered Approximate Optimal Tracking Control of Modular Reconfigurable Robots Via Adaptive Dynamic Programming}, 
  year={2024},
  volume={},
  number={},
  pages={536-541},
  abstract={This paper presents a decentralized self-triggered tracking control method for modular reconfigurable robots (MRRs) via adaptive dynamic programming (ADP). The Newton-Euler form dynamic modeling method is used to model the n- DOF MRR system. Based on self-triggering mechanism and local cost function, utilizing local critic neural network (NN) solve local Hamilton-Jacobi-Bellman (HJB) equation to obtain optimal tracking control. The self-triggered mechanism was introduced, which uses the system state at the triggering instant to predict the next triggering instant and update the control strategy, thereby reducing resource waste while avoiding the continuous moni-toring of system state under event-triggered conditions. Using Lyapunov theory for stability analysis ensures that the tracking error of the closed-loop MRR system is ultimately bounded (UUB). Finally, using 2-DOF MRR experimental platform to verify the proposed control is feasible.},
  keywords={Adaptation models;Trajectory tracking;2-DOF;Optimal control;Artificial neural networks;Mathematical models;Stability analysis;Dynamic programming;Robots;Monitoring;Adaptive dynamic programming;self-triggered mechanism;decentralized tracking control;optimal control;mod-ular reconfigurable robots},
