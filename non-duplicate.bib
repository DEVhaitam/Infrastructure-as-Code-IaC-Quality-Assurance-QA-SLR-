  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Asynchronous Quantization Control of Stochastic Markov Jump Systems with Round-Robin Protocol}, 
  year={2025},
  volume={},
  number={},
  pages={270-275},
  abstract={This paper investigates the asynchronous quantized control problem for stochastic Markov jump systems (SMJSs). A round-robin protocol is employed in the sensor-controller network to efficiently manage the transmission of measurement data from multiple sensor nodes. The measurement outputs are quantized using a logarithmic quantizer. A hidden Markov model characterizes the asynchrony between the plant and controller modes. By applying the delay transformation method and the equal-period sampling strategy, the SMJS under the round-robin protocol is transformed into the SMJS with time delay. A Lyapunov-Krasovskii functional is then constructed, and stochastic analysis techniques are utilized to derive a criterion for mean-square exponential stability. Finally, the effectiveness of the proposed method is demonstrated through a numerical simulation.},
  keywords={Protocols;Quantization (signal);Stability;Delay effects;Stochastic processes;Hidden Markov models;Numerical simulation;Delays;Control theory;Data communication;Stochastic Markov jump system;round-robin protocol;asynchronous control;logarithmic quantizer},
  doi={10.1109/CSIS-IAC65538.2025.11161382},
  ISSN={},
  month={May},}@INPROCEEDINGS{10919404,
  author={Zhang, Xiangbo},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={State-Feedback Control of a Class of Nonlinear Systems with Neutral Delays}, 
  year={2024},
  volume={},
  number={},
  pages={647-651},
  abstract={This paper solves the stabilization control problem for a class of strict-feedback nonlinear systems with neutral delays. First, a state transformation is introduced to equivalently convert the original system into a new system with an undetermined gain. Then, a memoryless state-feedback controller is designed via the gain design method. It is proved by the Lyapunov analysis method that the proposed controller guarantees that the closed-loop states are globally bounded and asymptotically converge to the equilibrium point. Compared with the existing works, the considered systems are more general, since both neutral delays and system nonlinearities are taken into account. Moreover, the tedious iterative procedures inherent in the backstepping design method are avoided in our control scheme. Finally, a simulation example is provided to demonstrate the effectiveness of our control strategy.},
