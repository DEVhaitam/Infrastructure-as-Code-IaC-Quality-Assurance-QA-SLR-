  month={May},}@INPROCEEDINGS{10919283,
  author={Lv, Yongfeng and Jian, Long and Zhao, Jun and Huang, Yingbo},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Adaptive Optimal Prescribed Performance Solutions for Multi-player Systems with Tracking Error Constraints}, 
  year={2024},
  volume={},
  number={},
  pages={183-187},
  abstract={This paper presented an RL-based robust controller design for the unknown servo system with disturbance. The system was identified with a three-layer NN. The robust controller is designed based on a zero-sum game theory, where the worst disturbance is studied to make robust control with certain anti-interference redundancy. A novel gradient law is developed to learn the observer NN parameter. Moreover, the identifier convergence and the stability of the robust controller are proved. Finally, the servo model is simulated with the proposed robust controller. The theorem and the conclusions indicate that the proposed RL-based robust controller can provide a good performance of the servo system.},
  keywords={Adaptive systems;System dynamics;Artificial neural networks;Stability analysis;Dynamic programming;Trajectory;Steady-state;Servomotors;Game theory;Feedforward systems;Adaptive dynamic programming;tracking control;error constraint;prescribed performance;nonzero-sum game},
  doi={10.1109/CSIS-IAC63491.2024.10919283},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{11161793,
  author={Li, Jiaqi and Li, Yuan and Liang, Yuling and Ma, Junguan and Chen, Junyan},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={ADP-Based Finite-Horizon Control for Discrete-Time Systems with Multi-Inputs and Actuator Saturations}, 
  year={2025},
  volume={},
  number={},
  pages={561-566},
  abstract={In this article, a neural network (NN) methodology is raised to resolve the optimum control issue for affine nonlinear systems with multi-input constraints over a finite-horizon. Unlike the value and policy iterations in conventional approximation dynamic programming (ADP) technology, which usually require a sufficient amount of iterations and multiple iteration loops to ensure the steadiness of the system and the convergence of system states and control strategies, a finite-horizon constrainedinputs optimum control methodology relying on the actor-critic structure is presented. This method can be utilized as time goes forward. The final cost function is considered, although the system state values converge to zero in the finite-horizon issue. The stability of the presented control algorithm is examined by means of the Lyapunov stability principle. Ultimately, a simulation case is provided to demonstrate the feasibility and effectiveness of the devised algorithm.},
  keywords={Discrete-time systems;Actuators;Heuristic algorithms;Artificial neural networks;Control systems;Stability analysis;Mathematical models;Dynamic programming;Nonlinear systems;Lyapunov methods;finite-horizon optimum control;approximate dynamic programming;multi-inputs;neural network (NN);hamilton-jacobi-bellman (HJB) equation},
  doi={10.1109/CSIS-IAC65538.2025.11161793},
  ISSN={},
  month={May},}@INPROCEEDINGS{10363868,
  author={Liu, Yanchao and Dou, Quansheng and Tang, Huanling and Zhou, Liyuan and Li, Xiujuan},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Certifiably Robust Out-Of-Distribution Detection Based on Prediction Uncertainy Weighting}, 
  year={2023},
  volume={},
  number={},
  pages={69-74},
  abstract={The application of machine learning in safety-critical systems requires a reliable assessment of uncertainy. However, deep neural networks are known to produce highly overconfident predictions on out-of-distribution (OOD) data. Even if trained to be non-confident on OOD data one can still adversarially manipulate OOD data so that the classifier again assigns high confidence to the manipulated data. In this paper, we improve the existing guaranteed OOD detection GOOD, and propose Certifiably Robust Out-of-distribution Detection based on Prediction Uncertainy Weighting (PUWOOD). Due to the loose boundary of GOOD, it is obviously not robust enough to deal with adversarial OOD close to the boundary of the decision. Therefore, the evaluation and weighting based on prediction uncertainty enable the model to achieve importance ranking in the face of different samples, and make it focus on key samples. Through experiments, we achieve the detection on adversarial OOD data while maintaining good performance on the original classification task and detecting clean data, which proves that our method has better performance in both robustness and accuracy.},
  keywords={Training;Uncertainty;Computational modeling;Machine learning;Predictive models;Robustness;Task analysis;Out-of-distribution detection;Adversarial robustness;Certified robustness;Adversarial examples},
  doi={10.1109/CSIS-IAC60628.2023.10363868},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10364141,
  author={Shao, Zhi and Liang, Yuling and Xing, Jin and Su, Hanguang},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Event-Triggered Decentralized Guaranteed Cost Control for Interconnected Large-Scale System Using Adaptive Dynamic Programming}, 
  year={2023},
  volume={},
  number={},
  pages={635-640},
  abstract={A novel decentralized guaranteed cost control (GCC) method for interconnected large-scale systems is proposed by utilizing the event-triggered adaptive dynamic programming (ADP) approach. First, the solution of GCC issue can be achieved by converting it into zero-sum differential game problem of sub-systems based on the ADP algorithm. An improved value function is designed and the guaranteed cost (GC) can be acquired by solving the Hamilton-Jacobi-Bellman (HJB) equation. Second, to reduce the execution frequency of control tasks and decrease the consumption of communication and computing loads, an event-triggered ADP-based decentralized GC approach is developed for interconnected systems with uncertainties. Finally, simulation results are given to show the feasibility of the proposed algorithm.},
  keywords={Costs;Adaptive systems;Uncertainty;Heuristic algorithms;Differential games;Control systems;Stability analysis;event-triggered control (ETC);guaranteed cost control;Hamilton-Jacobi-Bellman;adaptive dynamic programming},
  doi={10.1109/CSIS-IAC60628.2023.10364141},
