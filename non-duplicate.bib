  author={Hidayat, Nurul and Wardoyo, Retantyo and Azhari, SN},
  booktitle={2018 Third International Conference on Informatics and Computing (ICIC)}, 
  title={Educational Data Mining (EDM) as a Model for Students' Evaluation in Learning Environment}, 
