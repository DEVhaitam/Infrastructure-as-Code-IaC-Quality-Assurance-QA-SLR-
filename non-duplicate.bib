  author={Rahmat, Romi Fadillah and Silviani, Silviani and Nababan, Erna Budhiarti and Sitompul, Opim Salim and Anugrahwaty, Rina and Silmi, Silmi},
  booktitle={2017 Second International Conference on Informatics and Computing (ICIC)}, 
  title={Identification of molar and premolar teeth in dental panoramic radiograph image}, 
