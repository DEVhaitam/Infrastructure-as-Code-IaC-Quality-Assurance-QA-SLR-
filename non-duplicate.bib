  month={May},}@INPROCEEDINGS{10919335,
  author={Li, Yuepeng},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Research on the Application of Evolutionary Game Theory in Port Safety Information System}, 
  year={2024},
  volume={},
  number={},
  pages={879-884},
  abstract={The purpose of this study is to explore the application of evolutionary game theory to the construction of port safety information systems, and to construct a dynamic model to characterize the evolution of this strategy by analyzing the game behaviors of various stakeholders in the process of safety equipment utilization and cost control. The research methods mainly include theoretical model construction, mathematical derivation and case analysis, focusing on the interaction and influencing factors of different strategies between port management and transportation companies. The research results show that reasonable investment strategies can effectively reduce the probability of safety incidents, promote the gradual adoption of safety equipment by transportation companies, and enable the port safety system to be fully optimized and operate stably. The conclusion is that evolutionary game theory is a strong theoretical support and quantitative basis for port safety management, which can help all participating entities achieve a balance of interests and efficient operation of the system, and can be used for port safety management in complex environments.},
  keywords={Analytical models;Safety management;Transportation;Companies;Games;Mathematical models;Stakeholders;Game theory;Information systems;Investment;component;Evolutionary game theory;port safety;information system;strategy evolution},
  doi={10.1109/CSIS-IAC63491.2024.10919335},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10919376,
  author={Wang, Jialei and Sheng, Jinhua and Zhong, Haowen and Chen, Zhouqi and Lin, Jiaqi and Ying, Ziyi},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Predicting Cognitive Level with Resting-state Functional Connectivity for Alzheimer's Disease}, 
  year={2024},
  volume={},
  number={},
  pages={676-681},
  abstract={Neuroimaging is an effective and objective method to assess cognitive levels, minimizing bias from subjective measurements and identifying relevant neural markers. In this study, we proposed a novel data-driven cognitive prediction framework called ensemble connectome-based relevance-vectors-regression predictive modeling (ECRPM). After obtaining the relevant resting-state functional magnetic resonance imaging (fMRI), we applied the previously proposed joint human connectome project multi-modal parcellation (J-HCPMMP) method to derive the resting-state functional connectivity (rs-FC), which is an undirected graph represents 360 brain regions, then extracted connections between brain regions related to cognition using the significant edge selection (SES) method. Following dimensionality reduction, the feature data was combined with the connectome and cognitive measurements using the relevance vector regression (RVR) method. To enhance the robustness and predictive power of the model, we used an ensemble learning framework, simultaneously trained multiple homogeneous models and assigned appropriate weights to each base model based on their predictive abilities. Using this framework to predict ADASt3 scores, we found strong correlations between the predicted and observed cognitive scores. The positive network was more effective in predicting ADASt3 scores (r=O.780). To further verify the model's generalization ability, we employed k-fold cross-validation and leave-one-out cross-validation methods. The results demonstrated the framework's strong cognitive prediction ability. Finally, analysis of the feature data revealed core brain regions related to cognitive decline, particularly the entorhinal cortex (EC) region, which aligns closely with previous studies.},
  keywords={Neuroimaging;Image edge detection;Predictive models;Functional magnetic resonance imaging;Brain modeling;Feature extraction;Vectors;Robustness;Object recognition;Alzheimer's disease;Alzheimer's disease (AD);cognitive level prediction;ensemble conn ectom e-based relevance-vectors-regression predictive modeling (ECRPM);resting-state functional connectivity (rs-FC);machine learning},
