  abstract={A method is presented to process functional Magnetic Resonance Imaging (fMRI) data acquired in a Variable Attentional Control Task Experiment (VACT) to study activation properties in patients affected by schizophrenia. The method is based on the Canonical Correlation Analysis (CCA) and allows to process data of groups of patients by performing a group analysis (e.g. by means of the ANOVA method).},
  keywords={Correlation;Transmission line matrix methods;Analysis of variance;Magnetic resonance imaging;Process control;Physiology;Magnetic Resonance Imaging (MRI);functional MRI (fMRI);Canonical Correlation Analysis (CCA)},
  doi={10.1109/MeMeA.2011.5966730},
  ISSN={},
  month={May},}@ARTICLE{9480402,
