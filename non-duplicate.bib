  month={Oct},}@INPROCEEDINGS{11160758,
  author={Chen, Junrui and Shen, Zhen and Zhu, Yilin and Xiong, Gang and Liu, Xiwei},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
