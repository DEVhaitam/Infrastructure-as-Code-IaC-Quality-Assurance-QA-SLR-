  month={Sep.},}@INPROCEEDINGS{10822355,
  author={Pierini, Elisa and Mazza, Ludovica and Petti, Manuela and Tieri, Paolo},
  booktitle={2024 IEEE International Conference on Bioinformatics and Biomedicine (BIBM)}, 
  title={Exploring Drug Repurposing Success Stories Through a Network-based Approach: Insights from a Case Study}, 
  year={2024},
  volume={},
  number={},
  pages={6959-6964},
  abstract={Drug repositioning is a promising strategy to discover new therapeutic applications for existing drugs, significantly reducing the time and costs associated with traditional drug development. This study employs a network medicine approach to analyze successful cases of drug repositioning, focusing on the exploratory hypothesis that the efficacy of repositioning may be determined by functional similarity between between diseases for which the drug was originally designed and diseases for which the same drug is reused. Network medicine tools were employed to investigate the connections between disease-associated genes, proteins, and approved drugs. Biological networks, including protein-protein interactions and functional interactions networks, as well as gene- and drug-disease association data are analyzed to identify functional similarities and possible molecular connections between diseases and treatments. Using clustering techniques and topological analysis, the results reveal a suggestive overlap of involved genes and functional interactions, emphasizing the value of computational methods in accelerating drug repositioning efforts and improving understanding of drug repositioning dynamics for more efficient therapeutic interventions.},
  keywords={Drugs;Proteins;Obesity;Costs;Knowledge based systems;Focusing;Machine learning;Biology;Diabetes;Diseases;drug repositioning;network analysis;graph theory;bioinformatics;gene disease association},
  doi={10.1109/BIBM62325.2024.10822355},
  ISSN={2156-1133},
  month={Dec},}@INPROCEEDINGS{10919440,
  author={Peng, Zhangbo and Zeng, Wei},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Detection of Freezing of Gait in Parkinson's Disease Based on Deep Neural Networks and Machine Learning Models}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={This paper introduces an innovative methodology for the detection of freezing of gait (FOG) in individuals with Parkinson's disease (PD) by leveraging wireless inertial sensors. Utilizing the Daphnet Parkinson's database, which provides three-axis six-degree-of-freedom acceleration data, we present an advanced approach to address the intricate challenges associated with FOG detection. The methodology encompasses the extraction of acceleration time series data for both normal and freezing gaits through sliding window segmentation with varying widths of 1s, 2 $s$, 3s, and 4 $s$, each incorporating a fixed overlap ratio of 0.5. This deliberate variation in window widths aims to scrutinize the influence of temporal granularity on the efficacy of FOG detection. In the data preprocessing phase, we employ the Synthetic Minority Over-sampling Technique (SMOTE) to augment the FOG dataset, thereby bolstering the robustness of our model. The core of our methodology lies in the application of a deep neural network (DNN) for feature extraction. In this pivotal phase, we introduce the Convolutional Block Attention Module (CBAM) to enhance the discriminative capabilities of the network. CBAM incorporates channel and spatial attention mechanisms, enabling the network to selectively focus on salient features within the acceleration time series data. For classification of normal and FOG episodes, nine machine learning classifiers are employed, achieving a remarkable accuracy rate of 100% using a 10-fold cross-validation method. This comprehensive approach not only showcases the effectiveness of the CBAM-enhanced DNN but also underscores the meticulous consideration of temporal granularity in optimizing FOG detection efficacy.},
  keywords={Wireless communication;Wireless sensor networks;Accuracy;Parkinson's disease;Inertial sensors;Time series analysis;Artificial neural networks;Feature extraction;Robustness;Monitoring;Parkinson's disease;freezing of gait (FOG);inertial sensors;deep neural network;feature extraction;SMOTE;Convolutional Block Attention Module (CBAM);machine learning},
  doi={10.1109/CSIS-IAC63491.2024.10919440},
