  month={Aug},}@INPROCEEDINGS{11147360,
  author={Hamza, Usman and Abdullah, Nasuha Lee and Mashita Syed-Mohamad, Sharifah},
  booktitle={2025 IEEE Integrated STEM Education Conference (ISEC)}, 
  title={Integrating DevOps Knowledge Using a University-Based Software Engineering Course in Nigerian Universities}, 
  year={2025},
  volume={},
  number={},
  pages={1-8},
  abstract={This paper provides a design and pilot implementation of a DevOps-based Software Development course learning process for computer science and software engineering students in Al-Qalam University, Nigeria. Part of proposed system is Body of Knowledge in DevOps technology for Software Engineering and Computer Science curriculum, which is based on expert opinions and describes the Knowledge Groups required for Software Engineering professionals to do their work and DevOps young software engineers. The Software Engineering DevOps fundamentals serve as a foundation for identifying critical skills and competencies, and building curriculum and profiles for those skills and competencies. The paper implements the first course that has been conducted at Al-Qalam University Katsina for semester I, 2023/2024 session and describes teaching techniques used to create courses, including project-based learning, which helps students understand DevOps and Agile development techniques and provides a brief introduction to commonly used DevOps terminology, concepts, models, and tools, with a focus on DevOps tools for software development, deployment, and operations, which support key DevOps development principles. Finally, the paper proposed a curriculum course outline Cloud computing and related services make it possible to employ Infrastructure as Code (IaCode) to integrate, deploy, and manage modern software as a service and other related issue. Cloud platforms (both commercial and open source, such as Amazon Web Services (AWS) and Microsoft Azure) provide numerous opportunities for application management [6], monitoring and measuring cloud resources. The cloud provides the fundamental software engineering environment for delivering Big Data apps, and the platform for developing agile Data Science apps that empower agile data management for teaching DevOps techniques in Nigerian Universities for undergraduate software engineering and computer science students which has never been in existence in Nigerian University Commission curriculum.},
  keywords={Knowledge engineering;Computer science;Cloud computing;DevOps;Web services;Terminology;Education;Software as a service;Software engineering;STEM;Software Engineering;Body of Knowledge;Teaching methodology;Learning DevOps Software Engineering},
  doi={10.1109/ISEC64801.2025.11147360},
  ISSN={2473-7623},
  month={March},}@INPROCEEDINGS{10363919,
  author={Chen, Jiawei and Yang, Min and Hu, Haifeng and Zhang, Yunong and Tan, Ning},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Theoretics of Zhang Neurodynamics Models for Position-Orientation Control of Soft Continuum Robots from Kinematic Modeling to Noise Suppression}, 
  year={2023},
  volume={},
  number={},
  pages={914-919},
  abstract={Compared with conventional redundant robots, soft continuum robots (simply termed soft robots or continuum robots) are more flexible, so they have broad application prospects in the future. Although there are a few studies focused on the position-orientation tracking control of soft continuum robots, most of those studies are based on continuous mod-els/algorithms and may have poor effectiveness in noisy environments. Generally, discrete models/algorithms are more friendly to the digital computer, chip, and so on. Hence, developing a discrete, effective, and robust model for the position-orientation tracking control of soft continuum robots is meaningful and significant. In this paper, we first introduce the kinematic modeling process about a three-section soft robot. Then, we use Zhang neurodynamics (ZN) method and develop a continuous ZN (CZN) model. By applying three time-discretization formulas to discretize the CZN model, three discrete ZN (DZN) algorithms are developed for convenient control purposes. We also consider noise situations. Through formulas and detailed theoretical analyses, we prove that three DZN algorithms have great abilities under noise situations.},
  keywords={Neurodynamics;Systematics;Service robots;Computational modeling;Noise reduction;Kinematics;Production;Soft continuum robots;position-orientation tracking control;Zhang neurodynamics (ZN);discrete algorithms;noise suppression},
  doi={10.1109/CSIS-IAC60628.2023.10363919},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10363955,
  author={Cao, Zhiwei and Zhang, Yuansheng and Jin, Xin and Zong, Hucheng and Wang, Dongfan and Liang, Guojie},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Simulating a Complex Giant System: A Case Study on Yellow River Basin}, 
  year={2023},
  volume={},
  number={},
  pages={744-751},
  abstract={The Yellow River is the second longest river in China and the longest turbid river in the world. The Yellow River basin forms a giant, complex system that is one of the cradles of human civilization; it is also a region where rapid economic and social development is taking place. However, the excessive exploitation of resources in the basin has caused instability in the basin system, which is now restricting sustainable social and economic development in the region. This poses a considerable challenge to a deep understanding of its dynamics, and an effective approach to systematically examining the development status and evolution of river system is urgently required. This system is based on information entropy and dissipative structure theory and determines the basin status by quantifying the amount of indicator information and the degree of order in the system by considering related trends. As a result, this evaluation system provides not only a basis for characterizing the status of the Yellow River basin and associated trends, but also the evidence to support the planning of strategic development, particularly the early stages of major projects.},
  keywords={Economics;Market research;Rivers;Planning;Indexes;Complex systems;Information entropy;Giant System;Yellow River;Basin Development Index (BDI);Entropy;Dissipative Structure},
  doi={10.1109/CSIS-IAC60628.2023.10363955},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{11162001,
