  ISSN={},
  month={May},}@INPROCEEDINGS{10363843,
  author={Zhang, Chan and Cang, Naimeng and Chen, Li and Jia, Zehua and Xue, Shan and Guo, Dongsheng},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Improved Zeroing Neural Network for Solving Time-Variant Linear Equation with Harmonic Noise}, 
  year={2023},
  volume={},
  number={},
  pages={909-913},
  abstract={Time-variant linear equation (TVLE) is commonly involved in the science and engineering. Zeroing neural network (ZNN) and its variants have been developed and exhibited remarkable performance (e.g., noise rejection and finite-time convergence) on solving the TVLE. In this paper, we delve into a more in-depth investigation by enhancing the ZNN model for solving TVLE in the presence of harmonic noise. Such a model enables calculation error tend to zero, thereby facilitating the identification of the exact TVLE solution. To verify the supremacy of this enhanced ZNN model, we conduct a rigorous evaluation, assessing its properties such as convergence and robustness through various illustrative examples.},
  keywords={Simulation;Neural networks;Harmonic analysis;Mathematical models;Robustness;Complex systems;Convergence;Zeroing neural network (ZNN);harmonic noise;time-varying linear equation (TVLE);simulations},
  doi={10.1109/CSIS-IAC60628.2023.10363843},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10364145,
  author={Ai, Ze and Zhang, Ren and Wang, Huanqing},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Adapitve Fixed-Time Fault-Tolerant Tracking Control for Nonlinear Systems with Unmodeled Dynamics}, 
  year={2023},
  volume={},
  number={},
  pages={214-219},
  abstract={This paper focus on the adaptive fixed-time fault-tolerant tracking control for strict feedback nonlinear systems with dynamic disturbances, unmodeled dynamics and actuator fault. A novel fixed-time dynamics signal method is designed can be used to deal with the fixed-time control problem from nonlinear systems with unmodeled dynamics. Then, combining adaptive backstepping method and fixed-time theory, an adaptive fixed-time control approach can be developed that can ensure all signals of the closed-loop system are fixed-time bounded. Finally, the simulation example is used to prove the effectiveness of designed method.},
  keywords={Fault tolerance;Actuators;Adaptive systems;Backstepping;Fault tolerant systems;Stability analysis;Nonlinear dynamical systems;fixed-time control;unmodeled dynamics;fault-tolerant control},
  doi={10.1109/CSIS-IAC60628.2023.10364145},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10364082,
  author={Rao, Yao and Zhu, Wenjun and Zhou, Botao and Feng, Jiajun and Liu, Zheng and Wei, Qinglai},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Spatial Temporal Graph Transformers for Probabilistic Load Forecasting}, 
  year={2023},
  volume={},
  number={},
  pages={977-980},
  abstract={Ensuring a balance between electricity supply and demand is crucial for effective energy generation and scheduling. To this end, accurate probabilistic energy prediction is essential. In this paper, we introduce a novel transformer-based framework, Spatial Temporal Graph Transformers (STGT), designed to estimate probability distributions for future time series data. By combining Graph Neural Networks (GNNs) with Transformers, STGT is able to extract both temporal and spatial features, ultimately resulting in more reliable and accurate probability predictions. The architecture of STGT consists of a transformer decoder to capture historical load data and a graph neural network to model the spatial relationships present across different user loads. Additionally, quantile regression is utilized to model load probability distributions. Through extensive experiments conducted on real-world load time series datasets, we demonstrate that STGT outperforms alternative approaches in both point prediction and probabilistic prediction accuracy.},
  keywords={Supply and demand;Load forecasting;Time series analysis;Transformers;Probabilistic logic;Feature extraction;Probability distribution;Pobabilistic load forecasting;transformers;graph neural networks;time series forecasting},
  doi={10.1109/CSIS-IAC60628.2023.10364082},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10364090,
  author={Jin, Yuyue and Su, Hanguang and Liu, Fan and Cui, Yi and Zhi, Gan},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Event-Triggered Programming for H∞ Control of Completely Unknown Nonlinear System via Generalized Fuzzy Hyperbolic Models}, 
  year={2023},
  volume={},
  number={},
  pages={890-895},
  abstract={For the infinite-horizon (H∞) optimal control problem of a nonlinear system with completely unknown dynamics, a generalized fuzzy hyperbolic model-based identifier is designed to reconstruct the system dynamics. By the means of Lyapunov method, the uniform ultimate boundedness (UUB) stability of the identifier state estimation error is proved under the designed parameter regulation law. The value function is approximated by using an adaptive critic design with a single network structure to obtain an approximate optimal solution to the Hamilton-Jacobi-Isaacs (HJI) equation. Static and dynamic event-triggered near-optimal controllers with dead zones are designed, respectively. In this work, the triggering conditions only depend on the system state, and both control and disturbance strategies are based on the event-triggered conditions. Finally, the effectiveness of the proposed method for the system dynamics is verified by a numerical example.},
  keywords={Adaptation models;Adaptive systems;System dynamics;Heuristic algorithms;Approximation algorithms;Mathematical models;Stability analysis;Adaptive dynamic programming (ADP);generalized fuzzy hyperbolic models(GFHMs);infinite-horizon (H∞) control;zero-sum games},
  doi={10.1109/CSIS-IAC60628.2023.10364090},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{11161283,
  author={Yan, Shipeng and Liu, Guoliang and Jiang, Tonglei and Chen, Huili and Zhang, Tenglong and Guo, Qingqiang},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={A Predictive Optimization Framework for Stability-Constrained Obstacle Avoidance and Anti-Swing Motion Planning of Overhead Cranes}, 
  year={2025},
  volume={},
  number={},
  pages={213-219},
  abstract={In this paper, we propose an enhanced motion planning framework for overhead cranes that ensures both stability and obstacle avoidance. The Time-Elastic Band (TEB) method is widely recognized for its efficiency in real-time path planning, offering adaptive trajectory optimization and fast replanning capabilities in dynamic environments. However, it does not account for payload swing, making it unsuitable for crane operations where stability is crucial. To overcome this limitation, we integrate Model Predictive Control (MPC) with TEB, explicitly incorporating nonzero payload swing angles into the optimization process. This fusion enables the crane to achieve coordinated anti-swing control and obstacle avoidance, ensuring safe and efficient motion even in dynamic environments. Simulation results demonstrate that the proposed framework effectively reduces payload oscillations while maintaining robust obstacle avoidance, significantly enhancing the operational safety and performance of overhead cranes.},
  keywords={Cranes;Simulation;Dynamics;Path planning;Stability analysis;Planning;Safety;Collision avoidance;Trajectory optimization;Payloads;bridge crane;obstacles;Time-Elastic Band;antiswing;model prediction control},
  doi={10.1109/CSIS-IAC65538.2025.11161283},
  ISSN={},
  month={May},}@INPROCEEDINGS{11160875,
  author={Dang, Boyu and Li, Haiyan},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Iterative Learning Control Method with Safety Velocity of Mobile Robot Under Uncertain Motion Environment}, 
  year={2025},
  volume={},
  number={},
  pages={696-701},
  abstract={To improve the tracking accuracy and safety of mobile robot, a safety velocity iterative learning control method is proposed. The dynamic model of the mobile robot is established by considering the mass of the goods handled and the eccentricity of the goods placed. The saturation function is used to constrain the specified velocity of the robot, and the tracking error system is constructed. Furthermore, the stability of the tracking error system is analyzed based on Lyapunov theory. The simulation results show that the proposed control method restrains the influence of the uncertain environment on the robot movement, improves the robustness of the trajectory tracking and guarantees the safety of the motion velocity.},
  keywords={Tracking;Trajectory tracking;Simulation;Dynamics;Stability analysis;Robustness;Safety;Mobile robots;Complex systems;Iterative learning control;mobile robot;iterative learning control;safety velocity;uncertain motion environment},
  doi={10.1109/CSIS-IAC65538.2025.11160875},
  ISSN={},
  month={May},}@INPROCEEDINGS{10363796,
  author={Jiang, Gao-Feng and Wang, Su-Mei and Ni, Yi-Qing},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Deep Neural Network Based Suspension Control Failure Identification of Maglev Systems}, 
  year={2023},
  volume={},
  number={},
  pages={971-976},
  abstract={The comfort and stability of maglev systems are crucial to passenger safety and riding experience. Among them, the suspension gap between the bogie and the rail is one of the critical factors that determine comfort and stability. However, the structural disturbance may cause unstable control of the suspension gap. To address this problem, this study aims to analyze and identify the suspension control failure in a deep neural network approach. An array of accelerometers is installed on the rail to capture the structural response caused by suspension control failure. With the collected acceleration data, a neural network-based classification algorithm is proposed to distinguish the normal and failed suspension control status. As a result, the feature pattern between the normal and failed suspension control can be found in the collected acceleration data, and the proposed algorithm can reasonably identify the suspension control failure. Therefore, this study will help improve the failure identification of maglev systems, as well as the reliability and safety of maglev systems and the comfortable ride experience for passengers.},
  keywords={Rails;Artificial neural networks;Life estimation;Control systems;Stability analysis;Safety;Classification algorithms;suspension control;suspension failure detection;maglev systems;deep neural networks},
  doi={10.1109/CSIS-IAC60628.2023.10363796},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10364026,
  author={Jiao, Shanshan and Wei, Qinglai},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Parallel Control for Robust Optimal Tracking via Adaptive Dynamic Programming}, 
  year={2023},
  volume={},
  number={},
  pages={959-964},
  abstract={This paper attempts to develop a novel robust optimal tracking control method for continuous-time uncertain nonlinear systems via adaptive dynamic programming (ADP). First, a control system for the actual system is established based on the parallel control scheme, so that the general nonlinear system can be transformed into an affine system to introduce the optimal tracking control problem by defining an augmented vector. Then, the main objective of this paper is to investigate the relationship between the solutions of the optimal control problems for the uncertain system and the nominal system. Furthermore, critic neural networks (NNs) are utilized to design the virtual control law to achieve the proposed robust optimal control online. Finally, simulation experiments validate the effectiveness of the proposed method.},
  keywords={Uncertain systems;Adaptive systems;Optimal control;Artificial neural networks;Stability analysis;Dynamic programming;Nonlinear systems;Adaptive dynamic programming (ADP);robust optimal control;parallel control;tracking control;neural network (NN)},
  doi={10.1109/CSIS-IAC60628.2023.10364026},
  ISSN={},
