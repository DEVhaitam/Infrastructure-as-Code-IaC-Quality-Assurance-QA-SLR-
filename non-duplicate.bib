  author={Budiman, Indra and Herlianto and Suhartono, Derwin and Purnomo, Fredy and Shodiq, Muhsin},
  booktitle={2016 International Conference on Informatics and Computing (ICIC)}, 
  title={The effective noise removal techniques and illumination effect in face recognition using Gabor and Non-Negative Matrix Factorization}, 
