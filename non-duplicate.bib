  ISSN={},
  month={Nov},}@INPROCEEDINGS{8780499,
  author={Siagian, Mona Sarito and Valentine, Vega and Musa, Purnawarman},
  booktitle={2018 Third International Conference on Informatics and Computing (ICIC)}, 
  title={Comparison of Color Constancy Approaches on Images with Unbalanced Color Distribution}, 
  year={2018},
  volume={},
  number={},
  pages={1-6},
  abstract={Color images often suffer from degradations due to the acquisition and condition of light. Some of the problems are under-exposure and color cast which can be categorized as unbalanced color distribution. Two approaches of color constancy used to remove the unwanted color cast of test images; they are Gray-world assumption and Histogram matching. This paper discusses the difference between the two approaches and compares the result images. Both approaches can successfully remove color casts of the images with different characteristic of the result image.},
  keywords={Image color analysis;Histograms;Lighting;Color;Manganese;Gray-scale;Cameras;color constancy;unbalanced color distribution;color cast;gray-world assumption;histogram matching},
  doi={10.1109/IAC.2018.8780499},
