  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Res-SRNN: Residual Spiking Recurrent Neural Network for Efficient Video Recognition}, 
  year={2024},
  volume={},
  number={},
  pages={359-365},
  abstract={Action recognition in videos is a critical task in computer vision, with a wide range of applications including security surveillance, behavior analysis, and cooperative control. Despite significant advancements in action recognition accuracy achieved by existing video recognition methods, they commonly suffer from high computational complexity and substantial consumption of memory and computational resources. To address the urgent demand for concise, energy-efficient, and high-performance video recognition methods, we propose a residual spiking recurrent neural network (Res-SRNN). Firstly, we design the residual convolution module based on 2D convolution operations to effectively extract spatial information from input video frames. Moreover, drawing inspiration from the recurrent connections in the visual cortex, we incorporate the microscale spiking mechanism of the brain and put forward the spiking recurrent neural network (SRNN). Our SRNN not only excels at capturing complex temporal dependencies but also conserves computational energy costs. Experimental results demonstrate that our proposed Res-SRNN method significantly outperforms most comparative methods on benchmark datasets, achieving the best balance between parameter efficiency and performance.},
  keywords={Visualization;Recurrent neural networks;Costs;Accuracy;Convolution;Surveillance;Energy efficiency;Spatiotemporal phenomena;Data mining;Videos;Video Recognition;Spiking Mechanism;Recurrent Connection},
  doi={10.1109/CSIS-IAC63491.2024.10919427},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10919439,
  author={Lin, Qing and Li, Ting and Li, Yiran and Luo, Zong-Fu},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Risk Assessment of Satellite Constellations Using Network Theory}, 
  year={2024},
  volume={},
  number={},
  pages={522-529},
  abstract={The increasing satellite population in low Earth orbit heightens collision risks and challenges the traditional space situational awareness methods. These methods, typically focused on binary interactions, do not fully capture the complexities of satellite constellation dynamics, especially for smaller objects. This study introduces an innovative satellite constellation safety assessment approach that integrates the collision probability algorithm DilutionMaxPc with complex network analysis. We applied this methodology to actual satellite data and simulated constellation configurations, including objects smaller than 10 cm. This demonstrates its capability to pinpoint critical network nodes and potential collision pathways. Our results indicate that this approach effectively combines intricate interactions and long- term orbital dynamics. It utilizes network centrality metrics alongside collision probabilities, providing a robust framework to enhance the accuracy and resilience of satellite constellation safety assessments.},
  keywords={Space vehicles;Satellite constellations;Satellites;Accuracy;Heuristic algorithms;Complex networks;Prediction algorithms;Orbits;Safety;Resilience;Satellite constellations;collision probability;orbital propagation;network centrality;space debris;satellite safety},
