@inproceedings{10.5555/963600.963628,
author = {Gong, Yun and Marcinkevicius, Tadas},
title = {Video transmission over a GSM wireless network},
year = {2003},
publisher = {Trinity College Dublin},
abstract = {Transmitting video over bandwidth-limited and error-prone mobile wireless channels is challenging. In this paper, we investigate the transmission of H.263-based video over a GSM wireless network. A fourth order Markov model based alogrithm proposed in [1] is used to generate burst errors on the block or packet level for the GSM wireless channel. We evaluate the transmission performance of H.263 video over the channel by simulation. An Intra-coding based error resilient method and a simple generic forward error correction scheme are discussed. Since the Markov-based algorithm can be generalized to model different error statistics, e.g., different wireless channels, the performance analysis of H.263-based video over the GSM network may be applied for other wireless channels such as 3G systems.},
