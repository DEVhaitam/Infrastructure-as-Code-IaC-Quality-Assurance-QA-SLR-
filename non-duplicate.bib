  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Transmission Tower Detection Scheme Based on STPP-YOLO Network}, 
  year={2025},
  volume={},
  number={},
  pages={684-689},
  abstract={Automated inspection of transmission line infrastructure using robots or drones is an important way to ensure transmission safety. The automatic detection of transmission tower is the premise of power grid inspection automation. In this paper, a transmission tower detection scheme using a lightweight Swin Transformer-based PP-YOLO network (STTP-YOLO) is proposed. It can locate the tower target from aerial images while balancing the detection speed and accuracy. First, a dataset of transmission towers suitable for deep learning research is collected, and accurate labeling is completed manually to improve the study of the model. Second, a new tower backbone network is proposed to replace the ResNet50vd-DCN in the original model by combining the swin transformer with the traditional convolutional neural network, and the model can extract the tower information more accurately. Then, the Feature Pyramid Network (FPN) is used for decoding layer construction to perform simple and fast multi-scale feature fusion to extract target information more accurately before result prediction. Finally, the experimental results prove that the proposed scheme has strong competitiveness in accuracy and detection speed, which can be deployed in the task of automatic detection pole tower of power grid, and has a good guiding effect on the detection of auxiliary components of transmission tower.},
  keywords={Deep learning;Visualization;Accuracy;Poles and towers;Inspection;Feature extraction;Transformers;Power grids;Safety;Data mining;power line inspection;transmission tower detection;lightweight model;feature pyramid network;swin transformer},
  doi={10.1109/CSIS-IAC65538.2025.11161636},
  ISSN={},
  month={May},}@INPROCEEDINGS{10919447,
  author={Du, Wenqian and Yuan, Guoling and Lin, Mingduo and Zhao, Bo and Yu, Kuoyong and Lin, Qiao},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Event-Triggered Decentralized Stabilization for State-Constrained Nonlinear Interconnected Systems Using Adaptive Dynamic Programming}, 
  year={2024},
  volume={},
  number={},
  pages={819-824},
  abstract={In this paper, an event-triggered decentralized stabilization method based on adaptive dynamic programming (ADP) is proposed for nonlinear interconnected systems with constant-valued state constraints. By introducing a barrier function for coordinate transformation, the original system with state constraints is transformed into an unconstrained form. Then, with the developed cost functions for auxiliary subsystems, the decentralized stabilization problem of interconnected systems is transformed into a series of optimal control problems. Here-after, to obtain the event-triggered optimal control policies, the local policy iteration algorithm is investigated to solve the event-triggered Hamilton-Jacobi-Bellman equations (HJBEs) for auxiliary subsystems. The local critic neural networks (NNs) are employed to approximate the cost functions. Furthermore, the closed-loop nonlinear interconnected system and the weight estimation errors of local critic NNs are guaranteed to be uniformly ultimately bounded by a set of developed decentralized stabilizing policies. Finally, a numerical example is employed to validate the effectiveness of the proposed method.},
  keywords={Estimation error;Adaptive systems;Event detection;Simulation;Optimal control;Interconnected systems;Artificial neural networks;Cost function;Approximation algorithms;Dynamic programming;Adaptive dynamic programming (ADP);State constraints;Nonlinear interconnected systems;Decentralized stabilization;Event-triggered control},
