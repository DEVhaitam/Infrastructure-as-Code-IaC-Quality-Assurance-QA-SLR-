  number={},
  pages={489-496},
  abstract={In the field of medical image processing and analysis, automated lesion detection in arthritis is crucial for clinical diagnostic assistance and presents a challenging task. To enhance the performance of automated detection, we integrated a Criss-Cross attention module into the YOLOv8 model in object detection within X-ray images of hand joints, which was aimed at better capturing pathological features between hand joints. We analyzed the relationship between the two distinct methods of utilizing attention, specifically extracting global attention weights and extracting local attention weights, and the features exhibited by arthritis, comparing the impact of different attention mechanisms on arthritis lesion detection within the object detection task. Specifically, we validated our approach on large-scale medical datasets for Osteoarthritis (OA) and Rheumatoid Arthritis (RA), respectively. Experimental results demonstrate that utilizing attention weights to extract attention within specific local regions in the YOLOv8 model yields superior results in arthritis lesion detection compared to using no attention mechanism or extracting global attention weights.},
  keywords={YOLO;Pathology;Predictive models;Feature extraction;Arthritis;Lesions;Task analysis;YOLO;arthritis;object detection;attention mechanism},
  doi={10.1109/CSIS-IAC60628.2023.10364047},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10363880,
  author={Liu, Junhao and Wang, Zhuo},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Quadrotor UAV Control Algorithm Based on Active Disturbance Rejection Control}, 
  year={2023},
  volume={},
  number={},
  pages={31-36},
  abstract={The quadrotor Unmanned Aerial Vehicle (UAV) possesses various advantages, such as miniaturization, full autonomy, and a simple structure. However, its attitude control stability is challenging to ensure due to inherent characteristics, including underactuation, high coupling, nonlinearity, and susceptibility to disturbances. To address these issues, this paper conducts dynamic modeling of the quadrotor's actuators, attitude ring, and position ring, followed by controller design. A comparative study is performed between the ADRC control algorithm and the cascade PID algorithm, which is currently applied in mainstream flight control. Through rigorous simulation validation and real-world testing, the proposed approach achieves better control performance in terms of smoothness during startup, reduced overshoot, and enhanced disturbance rejection compared to the cascade PID algorithm. This optimization significantly improves the control stability of the quadrotor UAV.},
  keywords={Couplings;Robust control;Attitude control;Heuristic algorithms;Autonomous aerial vehicles;Stability analysis;Vehicle dynamics;quadrotor;dynamic modeling;cascade control;ADRC control},
  doi={10.1109/CSIS-IAC60628.2023.10363880},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10363913,
  author={Chen, Peiguang and Yu, Xuanduo and Li, Dongqi and Peng, Cong and Yuan, Ruihan and Han, Shunjie},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Implementation of Multi-dimensional Power Big Data Visualization Platform Based on Python}, 
  year={2023},
  volume={},
  number={},
  pages={948-952},
  abstract={With the rapid development of social economy, related energy companies and organizations have produced a large number of research results and information data, but due to different standards and different caliber, they have not been effectively integrated and applicated in-depth. Therefore, it is necessary to build a comprehensive energy information pool covering the whole industry and build a multi-dimensional information data display platform. This paper builds a multi-dimensional data display platform based on big data technology by relying on data collection and processing, real-time interaction of front and back end data, and data visualization technology. First of all, Requests, Pandas and other tool libraries in Python are used to achieve data collection, analysis and processing, so as to achieve information search and information intelligent collection. Secondly, the call function in Python is used to introduce the data that has been processed in the data acquisition program file and needs to interact with the front end is into this file, and then the real-time data interaction is realized by using Flask framework and Ajax technology. Finally, using the HTML standard markup language, CSS style set and Echarts gallery technology based on JavaScript web programming language, a multi-dimensional energy data visualization display platform is constructed.},
  keywords={Economics;Technological innovation;Data acquisition;Data visualization;Companies;Big Data;Real-time systems;Multi-dimension;Data acquisition;Data interacti on;Data visualization;Python technology},
