  month={Oct},}@INPROCEEDINGS{10364068,
  author={Li, Jin-Sheng and Li, Ji-Gong},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Odor Plume Direction Estimation via Gas Sensor Array}, 
  year={2023},
  volume={},
  number={},
  pages={936-941},
  abstract={The direction of the odor plumes is very helpful for robots to preform odor source localization task. To address issues of that wind measurement equipment is not able or convenient to carry on the robots, this paper proposes a method for estimating the incoming direction of the odor plume with a gas sensor array. Firstly, a model for estimating the incoming direction of the odor plume was established via the positional relationship between gas sensors in the sensor array. Secondly, by using the output responses of the gas sensors in the array, the time delay between gas sensors was calculated via cross-correlation method. Thirdly, the solution of the model was solved using the Levenberg-Marquette method. Finally, the results of gas sensor arrays in different distribution patterns indicate that our proposed method can estimate the incoming direction of odor plume effectively.},
  keywords={Location awareness;Preforms;Delay effects;Estimation;Robot sensing systems;Task analysis;Complex systems;odor plume;incoming direction estimation;gas sensor array;cross-correlation;Levenberg-Marquette method},
  doi={10.1109/CSIS-IAC60628.2023.10364068},
