  month={Sep.},}@INPROCEEDINGS{11161032,
  author={Zhao, Yuhang and Jiang, Xuejun and Zhang, Kai and Pan, Yu and Yang, Qinmin},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={A Transfer Learning-Augmented Physics-Informed Neural Network With Spatiotemporal Adaptation for Efficient Wind Field Reconstruction}, 
  year={2025},
  volume={},
  number={},
  pages={331-338},
  abstract={The efficient utilization of wind energy is hindered by the inherent variability and uncertainty of wind, necessitating the acquisition of accurate and detailed wind field data. Wind measurement technologies, such as Light Detection and Ranging (LiDAR), provide precise but sparse wind speed data. To address this problem, physics-informed neural network (PINN) has been employed to reconstruct wind fields by integrating measurement data with the Navier-Stokes (NS) equations. However, PINN faces three critical limitations, including optimization difficulty, challenges in handling long-term temporal dependencies, and diminished reconstruction accuracy in regions far from LiDAR coverage. This paper presents ReWind-PINN, a transfer learning-augmented physics-informed framework with spatiotemporal adaptation for efficient wind field reconstruction. Through the integration of models pre-trained on high-fidelity simulated wind data, our approach enhances PINN optimization efficiency, accelerates convergence, and improves reconstruction accuracy. We further propose a progressive temporal training strategy coupled with an adaptive residual weighting mechanism, designed to effectively address temporal dependencies while reinforcing physical consistency in measurement-sparse regions. Experimental validation confirms that ReWind-PINN delivers enhanced reconstruction accuracy with improved computational efficiency and stability. The framework demonstrates applicability across various PINN variants and different wind conditions, maintaining robust performance in the presence of measurement noise and variations in spatiotemporal resolution.},
  keywords={Accuracy;Laser radar;Neural networks;Transfer learning;Noise;Stability analysis;Mathematical models;Spatiotemporal phenomena;Noise measurement;Optimization;Wind field reconstruction;physics-informed neural network;transfer learning;spatiotemporal adaptation},
  doi={10.1109/CSIS-IAC65538.2025.11161032},
  ISSN={},
  month={May},}@INPROCEEDINGS{10364201,
  author={Liang, Mingming},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Optimal Impulsive Control for Continuous-Time Nonlinear Systems Based on ADP}, 
  year={2023},
  volume={},
  number={},
  pages={37-42},
  abstract={This paper focuses on designing the optimal impulsive controller of continuous-time nonlinear systems. A general-event-based dynamics (GD) is constructed to describe the state transition characteristics across the impulsive actions for the continuous-time nonlinear systems. Then, based on the GD, a new impulsive controller design scheme and an impulsive adaptive dynamic programming (IADP) algorithm are proposed, which possess strong generality and feasibility. Next, by introducing a novel policy updating mechanism an efficient IADP (EIADP) algorithm is established to make it more flexible to fit into the memory-limited computing devices, thus improving the realization efficiency of the ADP-based approaches. The developed methods are proved to converge to the optimal impulsive performance index functions and obtain the optimal impulsive controllers for the continuous-time nonlinear systems. Finally, a numerical study is provided to verify the effectiveness of the proposed IADP and EIADP algorithms.},
  keywords={Heuristic algorithms;Performance analysis;Dynamic programming;Nonlinear systems;Complex systems},
  doi={10.1109/CSIS-IAC60628.2023.10364201},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{8780467,
  author={Sari, Annisa Monicha and Hidayanto, A. Nizar and Purwandari, Betty and Budi, Nur Fitriah Ayuning and Kosandi, Meidi},
  booktitle={2018 Third International Conference on Informatics and Computing (ICIC)}, 
  title={Challenges and Issues of E-Participation Implementation: A Case Study of E-Complaint Indonesia}, 
  year={2018},
  volume={},
  number={},
  pages={1-6},
  abstract={The evolution of information, communication and technology (ICT) brings many benefits in the relationship between citizens and governments to offer better services in public sector. But, in the fact e-participation implementation faces many challenges and issues especially in developing countries. Digital divide, lack skilled people and poor citizen education becomes a factor that impede e-participation implementation. This study aims to explore and analysis challenge and issues of e-complaint implementation in Indonesia. The interview was conducted using semi-structured in two weeks with the expert and citizen. The selection of the expert and citizen as participant was conducted using purposive sampling. STOPE framework used over this study to analyze and to keep objective of the research in context. There are 10 sub-domains based on STOPE framework analysis as the challenges and issues of e-complaint in Indonesia. Those are political bureaucracy & commitment, promoting strategy, security & privacy, lack skilled people in organization, team coordination, lack of training in organization, poor citizen education, trust, digital divide and social culture. Challenges and issues of e-complaint in Indonesia can analyze to provide insight that can be used for developing better e-complaint platform in Indonesia.},
  keywords={Interviews;Electronic government;Education;Decision making;developing country;e-complaint;e-participation;ICTs;STOPE framework},
  doi={10.1109/IAC.2018.8780467},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{8280588,
  author={Sularto, Lana},
  booktitle={2017 Second International Conference on Informatics and Computing (ICIC)}, 
  title={Performance Evaluation of accounting information system for restaurants smes in JABODETABEK}, 
  year={2017},
  volume={},
  number={},
  pages={1-6},
  abstract={This study aims to evaluate the performance of implementation results of the accounting information system application named “Aplikasi POS Kuliner UKM” which has been specially built for restaurant SMEs in Jakarta, Bogor, Depok, Tangerang and Bekasi (JABODETABEK) over two years of development and implementation. From 179 restaurants in JABODETABEK surveyed at system requirements analysis stage, 15 restaurant SMEs selected for the implementation phase, but has yet to be evaluated. Therefore, it is necessary to evaluate the utilization of “Aplikasi POS Kuliner UKM” by selected restaurant SMEs to understand the shortcomings and conformity with the user requirements. This evaluation is measured using descriptive statistics, paired sample t-test, and Importance Performance Analysis (IPA) method. Results from paired sample t-test shows that already fit between the expectations and reality. Evaluation results using IPA known indicators that must be corrected, maintained, and ignored, it is also known that the performance of AIS already exceed consumer expectations on average. Additional survey results shows that the AIS has good facilities and satisfactory application performance.},
  keywords={Artificial intelligence;Manuals;Information systems;Information technology;Calculators;Companies;System Evaluation;Accounting Information Systems;Restaurants;Small and Medium-sized Enterprises},
