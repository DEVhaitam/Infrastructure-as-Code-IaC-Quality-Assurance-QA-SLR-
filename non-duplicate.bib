  month={May},}@INPROCEEDINGS{10363998,
  author={Liu, Fan and Su, Hanguang and Zong, Yi and Li, Yushuai and Jiang, He},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Event-Based Adaptive Dynamic Programming for H∞ Control Problem of Input-Constrained Nonlinear Systems with Its Application to Power Systems}, 
  year={2023},
  volume={},
  number={},
  pages={709-714},
  abstract={In this article, a novel event-based adaptive dynamic programming algorithm is developed to solve the infinite-horizon (H∞) control problem for continuous-time input-constrained nonlinear systems. The communication complexity and computation load of the system can be obviously reduced through the state-dependent triggering condition of the state and control strategy. Moreover, an event-triggered single-network adaptive critic method is utilized, with the aim of obtaining the approximate optimal solution of the Hamilton-Jacobi-Isaacs (HJI) equation. In the process of algorithm learning, only weights of the critic network need to be adjusted, and the number of updated weights can be reduced by eliminating the action network. By means of Lyapunov theory, the stability of the designed closed-loop control system and the convergence of weights of the critic network are demonstrated. Finally, a real power system simulation example is presented to verify the stability, the capability to suppress disturbance and the effectiveness of the proposed method.},
  keywords={Adaptive systems;Heuristic algorithms;Power system simulation;Power system stability;Control systems;Stability analysis;Mathematical models;adaptive dynamic programming (ADP);H∞ control;zero-sum game;event-based control;input constraints},
  doi={10.1109/CSIS-IAC60628.2023.10363998},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{11161531,
  author={Bi, Kewei and Tan, Fuxiao},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Distributed Reinforcement Learning for Intelligent Decision-Making}, 
  year={2025},
  volume={},
  number={},
  pages={490-495},
  abstract={Distributed Reinforcement Learning (DRL), as a cutting-edge method in intelligent decision-making, demonstrates significant potential in smart grids, autonomous driving, and intelligent unmanned systems owing to its decentralized architecture, efficient multi-agent coordination capability, and adaptability to complex dynamic environments. This paper systematically reviews the theoretical framework of DRL and its advancements in these domains. In smart grids, DRL effectively addresses the volatility challenges induced by highpenetration distributed energy resources (DERs) through modelfree optimization, substantially enhancing dynamic scheduling and real-time decision-making performance. For autonomous driving, the graph neural network-enhanced DRL framework resolves multimodal perception-decision fusion challenges, with simulations verifying its superior performance over conventional rule-based methods in complex traffic scenarios. Furthermore, DRL exhibits remarkable scalability in multi-agent cooperative control and autonomous decision-making for intelligent unmanned systems. This paper further analyzes critical challenges in current DRL applications, including communication efficiency, algorithmic scalability, and privacy protection, while proposing future research directions to advance DRL implementations in complex intelligent systems.},
  keywords={Scalability;Decision making;Reinforcement learning;Power system stability;Real-time systems;Stability analysis;Smart grids;Vehicle dynamics;Autonomous vehicles;Optimization;Distributed Reinforcement Learning;Intelligent Decision-Making;Autonomous Driving;Smart Grid;Unmanned Systems},
  doi={10.1109/CSIS-IAC65538.2025.11161531},
  ISSN={},
  month={May},}@INPROCEEDINGS{11161710,
  author={Liu, Wenzhi and Li, Zifu and Lei, Kai and Zheng, Hongqing},
  booktitle={2025 IEEE International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Multi-USV Adaptive Fixed-Time Robust Formation Tracking Control}, 
  year={2025},
  volume={},
  number={},
  pages={164-171},
  abstract={For the multi-unmanned surface vehicles (USVs) formation tracking control problem based on virtual leader trajectory generated by a ship model, this paper proposes a robust adaptive fixed-time sliding mode control scheme incorporating a fixed-time adaptive disturbance observer. Firstly, a fixed-time adaptive disturbance observer (FxADO) is designed to accurately estimate and compensate for time-varying composite disturbances arising from model parameter uncertainties and external environmental disturbances, thereby significantly enhancing the anti-disturbance performance of the multi-USVs formation system. Secondly, by integrating an adaptive estimation mechanism with non-singular fast terminal sliding mode control (NFTSMC), an adaptive fixed-time formation tracking control strategy is developed. This strategy not only ensures that the multi-USV system achieves the desired formation configuration within a predefined fixed time but also enhances the error convergence rate of the formation tracking system by employing adaptive techniques. Finally, numerical simulations are conducted to demonstrate the feasibility of the proposed control strategy.},
  keywords={Adaptation models;Uncertain systems;Adaptive systems;Disturbance observers;Stability analysis;Trajectory;Sliding mode control;Time-varying systems;Numerical stability;Convergence;Formation tracking control;Fixed-time adaptive disturbance observer;Non-singular fast terminal sliding mode control;Adaptive fixed-time control},
  doi={10.1109/CSIS-IAC65538.2025.11161710},
  ISSN={},
  month={May},}@INPROCEEDINGS{10364020,
  author={Tao, Zhicheng and Li, Chao and Yang, Qinmin},
  booktitle={2023 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Network Clustering-Based Multi-Agent Reinforcement Learning for Large-Scale Traffic Signal Control}, 
  year={2023},
  volume={},
  number={},
  pages={326-331},
  abstract={Reinforcement learning (RL) has proven successful in the field of traffic signal control (TSC) within urban traffic networks. Moreover, multi-agent RL (MARL) achieves large-scale TSC by distributing the global control to each local RL agent. However, as the scale of joint TSC continues to expand, MARL faces greater convergence challenges. In this paper, a method combining network clustering and MARL is proposed to address this issue. We apply the Normalized Cut (Ncut) algorithm to effectively partition a large-scale traffic network, and then adopt MADQN to complete TSC tasks for each subnetwork. Meanwhile, a novel reward function based on impedance has been adopted. The results demonstrate that our method has high control performance and could effectively alleviate congestion in large-scale network under peak-hour traffic conditions.},
  keywords={Heuristic algorithms;Clustering algorithms;Reinforcement learning;Partitioning algorithms;Impedance;Indexes;Task analysis;Traffic signal control;Multi-agent reinforcement learning;Network clustering;Deep Q-network},
  doi={10.1109/CSIS-IAC60628.2023.10364020},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10919424,
  author={Fan, Jianjun and Lai, Guanyu and Zhong, Huihui},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={Gradient-Based Inverse Control for Noncanonical Nonlinear Systems with Unknown Input Backlash}, 
  year={2024},
  volume={},
  number={},
  pages={753-758},
  abstract={Actuator backlash nonlinearity typically leads to delays, oscillations, and errors, thereby affecting the effective control of actuators in closed-loop systems. To suppress the effects of backlash, numerous adaptive backlash compensation schemes for nonlinear systems in canonical forms have been proposed. Due to the explicit relative degree of canonical systems, adaptive backlash inverse can be integrated with the backstepping frame-work, thus the adaptive backlash compensation problem for such systems can be quickly addressed. However, unlike the canonical system, the relative degree information of the noncanonical system is implicit, and its control gain is usually expressed as an uncertain control gain function. The combination of this function with the backlash nonlinearity makes handling backlash nonlinearity in noncanonical systems extremely challenging. To address this issue, this paper proposes an adaptive backlash inverse scheme to solve the backlash compensation problem with uncertain control gain functions. Additionally, by integrating this scheme with the gradient framework, an adaptive backlash inverse compensation control scheme is developed, ensuring that all signals in the noncanonical closed-loop system are bounded, and the system output asymptotically tracks the reference signal. Simulation results confirm the significant effectiveness and superiority of the proposed control scheme.},
  keywords={Actuators;Simulation;Neural networks;Stability analysis;Delays;Closed loop systems;Nonlinear systems;Adaptive control;Complex systems;Oscillators;backlash characteristics;noncanonical nonlinear systems;neural networks;adaptive control},
  doi={10.1109/CSIS-IAC63491.2024.10919424},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10919388,
  author={Cai, Yunyao and Ji, Mingrui and Yuan, Runze and Peng, Lihui and Zhang, Yi},
  booktitle={2024 International Annual Conference on Complex Systems and Intelligent Science (CSIS-IAC)}, 
  title={A Hierarchical Optimal Control for Vehicle Platoon Based on Bidirectional Error Prediction and Saturated Artificial Potential Fields}, 
  year={2024},
  volume={},
  number={},
  pages={515-521},
  abstract={The Intelligent Vehicle Infrastructure Cooperative Systems (i-VICS) have much positive impact on the cooperative and adaptive platoon control for connected-automated vehicles( CAVs) based on the communication between vehicles and infrastructure. This paper proposes a novel hierarchical control architecture for CAVs platoon under i-VICS, where the upper layer calculates the time headway errors and the lower layer implements an optimal control. To ensure the information sharing among all vehicles in platoon, the bidirectional information flow topology under i-VICS is adopted to enhance the feasibility of cooperation. The sliding window is then introduced to predict the headway errors existing in the vehicle platoon while the distributed controller is designed to implement the optimal control for vehicle platoons based on gradient descent of a saturated artificial potential field. The parameters of optimal controllers are searched based on the proposed coarse-grained optimization. Finally four scenarios in simulation are selected to verify the proposed approach and analyse the results of its effectiveness.},
  keywords={Measurement;Intelligent vehicles;Simulation;Decentralized control;Optimal control;Information sharing;Cooperative systems;Stability analysis;Topology;Optimization;i-VICS;Platoon control;Connected-automated vehicles},
  doi={10.1109/CSIS-IAC63491.2024.10919388},
